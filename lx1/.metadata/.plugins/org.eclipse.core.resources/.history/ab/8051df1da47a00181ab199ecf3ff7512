package lx1;

public class Lx1_5 {
		public static void main(String args[]) {
			Human h = new Human();
			if (h instanceof Human) {
				h.swim();
			}
			if (h instanceof Runner) {
				h.run();
			}
			if (h instanceof Run) {
				h.run();
			}
			if (h instanceof Swim) {
				h.swim();
			}
		}
}


